                },
                {
                    "falaDoProtagonista": true,
                    "disparaEvento": false,
                    "ouvirEvento": false,
                    "opcao": false,
                    "textoDialogo": "Por que Deus é seu pastor-",
                    "textoEscutarEvento": "",
